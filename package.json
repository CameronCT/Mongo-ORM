{
  "name": "@cameronct/mongo-odm",
  "version": "0.1.82",
  "description": "A very basic MongoDB ODM for Node.js",
  "homepage": "https://cameronct.github.io/Mongo-ODM",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "publish:public": "npm publish --access public",
    "prettier": "prettier --config .prettierrc './src/**/*.{ts,tsx}' './test/**/*.{ts,tsx}' --write",
    "lint": "eslint . --ext .ts",
    "docs": "jsdoc -c jsdoc.conf.json",
    "test": "jest --coverage --forceExit"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "orm",
    "odm",
    "object document mapping",
    "object relational mapping"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CameronCT/Mongo-ODM.git"
  },
  "bugs": {
    "url": "https://github.com/CameronCT/Mongo-ODM/issues"
  },
  "author": {
    "name": "CameronCT",
    "email": "contact@cameronct.com"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "better-docs": "^2.7.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-partial": "^1.0.1",
    "jsdoc": "^4.0.2",
    "mongodb": "^6.2.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.20.1"
  }
}
